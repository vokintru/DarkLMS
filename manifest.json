{
  "manifest_version": 3,
  "name": "DarkLMS",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://lms.yandex.ru/*"],
      "css": ["style.css"]
    }
  ],
  "action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
    }
  },
    "permissions": [
    "activeTab",
    "storage"
  ]
}
